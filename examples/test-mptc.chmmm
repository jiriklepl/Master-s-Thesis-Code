class C a b {
  m (auto (a) x, auto (b) y) -> auto (a);
}

instance C bits32 bits32 {
  m (auto u, auto w) {
    w = w + u;
    return (w);
  }
}

instance C bits32 ptr(auto) {
  m (auto u, auto w) {
    u = u * 4;
    return (u);
  }
}

foreign "C"
f(bits32 x, ptr(bits64) y) {
  x = m(x, y);

  return (x);
}

foreign "C"
g(bits32 x) {
  x = m(x, x);

  return (x);
}
