class Id a b {
  id (auto (a) x, auto (b) y) -> auto (a);
}

instance Id bits32 bits32 {
  id (auto u, auto w) {
    w = w * 2;
    return (w);
  }
}

instance Id bits64 bits64 {
  id (auto u, auto w) {
    w = w * 3;
    return (w);
  }
}

instance Id bits32 bits64 {
  id (auto u, auto w) {
    u = u * 4;
    return (u);
  }
}

f(bits32 x, bits64 y) -> bits32 {
  x = id(x, y);

  return (x);
}


g(bits32 x) -> bits32 {
  x = id(x, x);

  return (x);
}
